var signupErrorsList = document.querySelector('.signup-errors-list');

$(signupErrorsList).empty()

<% resource.errors.full_messages.each do |message| %>
    <% message.gsub!("can't", 'cannot') %>

    var errorMessage = document.createElement('li')
    errorMessage.innerHTML = "<%= message %>";

    signupErrorsList.appendChild(errorMessage)
<% end %>
